		Или ИмяТипа = "Неопределено"
		Или ИмяТипа = "Булево"
		Или ИмяТипа = "Строка"
		Или ИмяТипа = "Число"
		Или ИмяТипа = "Дата"
		Или ИмяТипа = "Null";
