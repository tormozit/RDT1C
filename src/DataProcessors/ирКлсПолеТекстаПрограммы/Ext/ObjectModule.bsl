				//НачальнаяСтрока = 0;
				//НачальнаяКолонка = 0;
				//СлужебноеПолеТекстаДолгое.ПолучитьГраницыВыделения(НачальнаяСтрока, НачальнаяКолонка,,);
				//КончитьОбработкуКоманды();
				//РазобратьТекущийКонтекст(,,, НачальнаяСтрока, НачальнаяКолонка,, Позиция1);
			КонецЕсли;
