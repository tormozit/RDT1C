// Функция - Вставить выбранное слово автодополнения
//
// Параметры:
//  СтрокаРезультата			 - 	 - 
//  ТаблицаТиповКонтекста		 - 	 - 
//  ПараметрЗакрытияПодсказки	 - 	 - 
// 
// Возвращаемое значение:
//  Число - ВидПовторногоВызоваАвтодополнения, 0 - не вызывать, 1 - автовызов, 2 - принудительное
//
Функция ВставитьВыбранноеСловоАвтодополнения(СтрокаРезультата, ТаблицаТиповКонтекста = Неопределено, ПараметрЗакрытияПодсказки = Истина) Экспорт 

	#Если Сервер И Не Сервер Тогда
		ПолеТекста = Обработки.ирОболочкаПолеТекста.Создать();
	#КонецЕсли
	ВидПовторногоВызоваАвтодополнения = 0;
	Вставка = ВставитьВыбранноеСловоГенератор(СтрокаРезультата,, ВидПовторногоВызоваАвтодополнения, ПараметрЗакрытияПодсказки);
	//Если ЗначениеЗаполнено(Вставка) Тогда
		УстановитьПризнакМодифицированностиФормы();
		ВосстановитьГраницыВыделенияПослеКоманды();
		СохранитьСтатистикуВыбораПодсказки();
		ПолеТекста.ФиксированноеВыделениеДвумерное = Неопределено;
	//КонецЕсли;
	Возврат ВидПовторногоВызоваАвтодополнения;
КонецФункции

//.
// Параметры:
//   СтрокаРезультата - ОбработкаТабличнаяЧастьСтрока.ирКлсПолеТекстаПрограммы.ТаблицаСлов - 
Функция ВставитьВыбранноеСловоГенератор(Знач СтрокаРезультата, Знач ДляАдаптера = Ложь, ВидПовторногоВызоваАвтодополнения = 0, ПараметрЗакрытияПодсказки = Истина, ФорматироватьТекст = Ложь,
		ЛиГенераторСПоглощениемНачалаСтроки = Ложь) Экспорт 
	Если Истина
		И ЯзыкПрограммы = 0
		И ТипЗнч(Конфигурация) = Тип("COMОбъект") 
	Тогда
		//! Конфигурация = 0 // Application {V83.Application}
		ВыделениеОдномерное = ПолеТекста.ВыделениеОдномерное();
		ПолеТекстаПрограммыКОМ = ПолеТекстаПрограммыКОМ(Истина);
		Результат = ПолеТекстаПрограммыКОМ.ВставитьВыбранноеСловоГенератор(СтрокаРезультата, ДляАдаптера, ВидПовторногоВызоваАвтодополнения, ПараметрЗакрытияПодсказки, ФорматироватьТекст);
		Если СтрокаРезультата.Слово = СловоГенераторЗапрос() Тогда
			ВставитьУказанныйШаблон(Результат, СокрЛ(мТекущаяСтрокаНачало));
		Иначе
			ВставитьУказанныйШаблон(Результат, мНачалоКонтекста);
		КонецЕсли;
		ЗаполнитьЗначенияСвойств(ЭтотОбъект, ПолеТекстаПрограммыКОМ, "мНачальнаяСтрока, мНачальнаяКолонка, мКонечнаяСтрока, мКонечнаяКолонка, мОткрытьСправкуПоПараметру");
		Возврат Результат;
	КонецЕсли;
	Если ДляАдаптера Тогда
		УказательКаретки = "<!>";
	Иначе
		УказательКаретки = "";
	КонецЕсли;
	ФорматироватьТекст = Ложь;
	Результат = "";
	РодительИСлово = РодительИСловоСтрокиТаблицыСлов(СтрокаРезультата);
	Слово = РодительИСлово.Слово; 
	СтруктураТипаКонтекста = РодительИСлово.СтруктураТипаКонтекста;
	ОписаниеОжидаемогоТипа = РассчитатьОжидаемыйТипВыражения();
	НепечатноеНачало = ирОбщий.НепечатноеНачалоЛкс(мТекущаяСтрокаНачало);
	Если Слово = СловоГенераторКодаЗаполненияСвойств() Тогда
		ЛиГенераторСПоглощениемНачалаСтроки = Истина;
		МассивВставки = Новый Массив;
		СловаПравые = АнализаторСПолнойТаблицейСловМетода(Истина, Истина).ТаблицаСлов.Выгрузить(Новый Структура("ТипСлова", ПеречТипСлова.Свойство));
		Если мЭтоТекстовыйЛитерал Тогда
			СловаЛевые = ТаблицаСлов.Выгрузить(Новый Структура("ТипСлова", ПеречТипСлова.Свойство));
			ОсновнойКонецСтроки = ");";
			ШаблонИнструкции = мТекущаяСтрокаНачало + "%1"", ";
			СгенерироватьИнструкцииСопоставленияСвойств(МассивВставки,, ОсновнойКонецСтроки, СловаЛевые, СловаПравые, ШаблонИнструкции);
		Иначе 
			ПараметрыЗаполнения = мПлатформа.НовыеПараметрыЗаполненияСлов(ПеречТипСлова.Свойство, БазовоеРасширениеКонфигурации(), мЯзыкПрограммы,,,,,,,,,,,,, Истина);
			СловаЛевые = мПлатформа.ТаблицаСловИзСтруктурыТипа(СтруктураТипаКонтекста, ПараметрыЗаполнения);
			ОсновнойКонецСтроки = ";";
			ШаблонИнструкции = мТекущаяСтрокаНачало + "%1 = ";
			СгенерироватьИнструкцииСопоставленияСвойств(МассивВставки, ОписаниеОжидаемогоТипа.ВыражениеПриемник, ОсновнойКонецСтроки, СловаЛевые, СловаПравые, ШаблонИнструкции);
		КонецЕсли;
		Если СловаЛевые.Количество() Тогда
			Результат = ирОбщий.СтрСоединитьЛкс(МассивВставки, Символы.ПС);
			ВставитьУказанныйШаблон(Результат, мТекущаяСтрокаНачало); 
			ФорматироватьТекст = Истина;
		КонецЕсли;
	ИначеЕсли Слово = СловоГенераторКодаСопоставитьСвойства() Тогда
		ЛиГенераторСПоглощениемНачалаСтроки = Истина;
		Если мЭтоТекстовыйЛитерал Тогда
			НепечатноеНачало = НепечатноеНачало + "|";
		КонецЕсли;
		ОсновнойКонецСтроки = "";
		ПрефиксВставки = "";
		ЛиЛогическоеВыражение = Ложь
			Или мЯзыкПрограммы = 1
			Или Не ирОбщий.СтрНачинаетсяСЛкс(СокрЛ(мТекущаяСтрокаНачало), ОписаниеОжидаемогоТипа.ВыражениеПриемник);
		Если ЛиЛогическоеВыражение Тогда
			// Это логическое выражение
			НепечатноеНачало = НепечатноеНачало + Символы.Таб + "И ";
			//ПрефиксВставки = Лев(мТекущаяСтрокаНачало, Найти(мТекущаяСтрокаНачало, ОписаниеОжидаемогоТипа.ВыражениеПриемник) - 1);
			//ПрефиксВставки = ПрефиксВставки + "ИСТИНА" + Символы.ПС;
		Иначе
			ОсновнойКонецСтроки = ";";
		КонецЕсли;
		ИмяЛевогоОбъекта = ирОбщий.ПервыйФрагментЛкс(ОписаниеОжидаемогоТипа.ВыражениеПриемник);
		ТаблицаТиповЛевая = ВычислитьТипЗначенияВыражения(ИмяЛевогоОбъекта);
		МассивВставки = Новый Массив;
		ШаблонИнструкции = НепечатноеНачало + ИмяЛевогоОбъекта + ".%1 = " + мРодительскийКонтекст + ".";
		ПараметрыЗаполнения = мПлатформа.НовыеПараметрыЗаполненияСлов(ПеречТипСлова.Свойство, БазовоеРасширениеКонфигурации(), мЯзыкПрограммы,,,,,,,,,,,,, Не ЛиЛогическоеВыражение);
		СловаЛевые = мПлатформа.ТаблицаСловИзСтруктурыТипа(ТаблицаТиповЛевая[0], ПараметрыЗаполнения);
		СловаПравые = ТаблицаСлов.Выгрузить(Новый Структура("ТипСлова", ПеречТипСлова.Свойство));
		СгенерироватьИнструкцииСопоставленияСвойств(МассивВставки, ОписаниеОжидаемогоТипа.ВыражениеПриемник, ОсновнойКонецСтроки, СловаЛевые, СловаПравые, ШаблонИнструкции);
		Если МассивВставки.Количество() Тогда
			Результат = ирОбщий.СтрСоединитьЛкс(МассивВставки, Символы.ПС);
			ВставитьУказанныйШаблон(Результат, мНачалоКонтекста); 
			ФорматироватьТекст = Истина;
		КонецЕсли;
	ИначеЕсли Слово = СловоГенераторКодаУдалитьВыбранныеЭлементы() Тогда
		ЛиГенераторСПоглощениемНачалаСтроки = Истина;
		АнализаторЛокальный = АнализаторСПолнойТаблицейСловМетода();
		Если ТаблицаСлов.НайтиСтроки(Новый Структура("Слово, ТипСлова", "Найти", ПеречТипСлова.Метод)).Количество() Тогда // - "Индекс" не подходит для массива
			Результат = 
			"Для <Индекс> = 1 - <Коллекция>.Количество() По 0 Цикл // Обратный обход
			|	<Элемент> = <Коллекция>[-<Индекс>];
			|	Если <?> Тогда
			|		<Коллекция>.Удалить(-<Индекс>);
			|	КонецЕсли;
			|КонецЦикла;";
			Результат = ирОбщий.СтрЗаменитьЛкс(Результат, "<Индекс>", АнализаторЛокальный.НовоеИмяПеременной("Индекс"));
		Иначе
			Результат = 
			"МассивДляУдаления = Новый Массив; 
			|Для Каждого <Элемент> Из <Коллекция> Цикл
			|	Если <?> Тогда
			|		МассивДляУдаления.Удалить(<Элемент>);
			|	КонецЕсли;
			|КонецЦикла;
			|Для Каждого <Элемент> Из МассивДляУдаления Цикл
			|	<Коллекция>.Удалить(<Элемент>);
			|КонецЦикла;";
		КонецЕсли;
		Результат = ирОбщий.СтрЗаменитьЛкс(Результат, "<Коллекция>", мРодительскийКонтекст);
		Результат = ирОбщий.СтрЗаменитьЛкс(Результат, "<Элемент>", АнализаторЛокальный.НовоеИмяПеременной("Элемент"));
		ВставитьУказанныйШаблон(Результат, мНачалоКонтекста); 
		ФорматироватьТекст = Истина;
	ИначеЕсли Слово = СловоГенераторКодаОбойтиЭлементы() Тогда
		ЛиГенераторСПоглощениемНачалаСтроки = Истина;
		Если СтруктураТипаКонтекста.ИмяОбщегоТипа = "РезультатЗапроса" Тогда
			Построитель = СтруктураТипаКонтекста.Метаданные; // ПостроительЗапроса
			ВыборкиИтогов = Построитель.Измерения;
			// Мультиметка250112_105531
			Результат = "<ВложеннаяВыборка>";
			ПредИмяВыборки = мРодительскийКонтекст;
			Для ИндексУровня = 0 По ВыборкиИтогов.Количество() Цикл
				Если ИндексУровня = ВыборкиИтогов.Количество() Тогда
					ВыражениеГруппировки = "";
					ИмяВыборки = "ВыборкаДетальные";
				Иначе
					УровеньИтогов = ВыборкиИтогов[ИндексУровня];
					ИмяКолонки = ТРег(УровеньИтогов.Имя);
					ИмяВыборки = "Выборка" + ИмяКолонки;
					ВыражениеГруппировки = """" + ИмяКолонки + """";
				КонецЕсли; 
				СозданиеПеременной = ИмяВыборки + " = " + ПредИмяВыборки + ".";
				Смещение = НепечатноеНачало + ирОбщий.СтрокаПовторомЛкс(Символы.Таб, ИндексУровня);
				ТекстУровняВложенный = СозданиеПеременной + "Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам, " + ВыражениеГруппировки + ");
				|" + Смещение + "Пока " + ИмяВыборки + ".Следующий() Цикл
				|" + Смещение + "	<ВложеннаяВыборка>
				|" + Смещение + "КонецЦикла;";
				Результат = ирОбщий.СтрЗаменитьЛкс(Результат, "<ВложеннаяВыборка>", ТекстУровняВложенный);
				ПредИмяВыборки = ИмяВыборки;
			КонецЦикла;
			Результат = ирОбщий.СтрЗаменитьЛкс(Результат, "<ВложеннаяВыборка>", "<?>");
		Иначе
			Если СтруктураТипаКонтекста.ИмяОбщегоТипа = "ТабличныйДокумент" Тогда
				Результат = 
				"Для НомерСтроки = 1 По <Коллекция>.ВысотаТаблицы Цикл
				|	Для НомерКолонки = 1 По <Коллекция>.ШиринаТаблицы Цикл
				|		Ячейка = <Коллекция>.Область(НомерСтроки, НомерКолонки);
				|		<?>
				|	КонецЦикла;
				|КонецЦикла;";
				Результат = ирОбщий.СтрЗаменитьЛкс(Результат, "<Коллекция>", мРодительскийКонтекст);
			ИначеЕсли СтруктураТипаКонтекста.ИмяОбщегоТипа = "ТекстовыйДокумент" Тогда
				Результат = 
