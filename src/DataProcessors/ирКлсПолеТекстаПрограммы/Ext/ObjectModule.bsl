// Отправляет запрос к ИИ API и возвращает ответ
// Параметры:
//  Текст        - Строка - Содержимое сообщения
//  КлючСервисаИИ      - Строка - Ключ API для аутентификации
//  БазовыйАдрес - Строка - URL-адрес API, опционально, по умолчанию - OpenRouter
//  Модель       - Строка - Название модели, опционально, по умолчанию - deepseek/deepseek-chat
// Возвращаемое значение:
//  Строка - Ответ от OpenAI или пустая строка
Функция ОтправитьЗапросOpenAI(Текст, КлючСервисаИИ = "", БазовыйАдрес = Неопределено, Модель = Неопределено)
	Если ПустаяСтрока(КлючСервисаИИ) Тогда
		#Если Клиент Тогда
			Форма = ирКлиент.ОткрытьНастройкиПоляТекстаПрограммыНаЗаднемПланеЛкс("", Истина);
			Форма.ТекущийЭлемент = Форма.ЭлементыФормы.КлючСервисаИИ;
		#КонецЕсли
		ВызватьИсключение "Не указан ключ сервиса ИИ";
	КонецЕсли;
	ТекБазовыйАдрес = ?(ЗначениеЗаполнено(БазовыйАдрес), БазовыйАдрес, "https://openrouter.ai/api/v1/chat/completions");
	ТекМодель = ?(ЗначениеЗаполнено(Модель), Модель, "deepseek/deepseek-chat:free");
	РазобранныйURL = ирОбщий.СтруктураURIЛкс(ТекБазовыйАдрес);
	HTTPСоединение = ирОбщий.СоединениеHTTPЛкс(РазобранныйURL.Хост, РазобранныйURL.Порт,,,, 120, РазобранныйURL.Схема = "https");
	HTTPЗапрос = ирОбщий.ЗапросHTTPЛкс(РазобранныйURL.ПутьНаСервере);
	HTTPЗапрос.Заголовки.Вставить("Content-Type", "application/json");
	Если ЗначениеЗаполнено(КлючСервисаИИ) Тогда
		HTTPЗапрос.Заголовки.Вставить("Authorization", "Bearer " + КлючСервисаИИ);
	КонецЕсли;
	СтруктураЗапроса = Новый Структура;
	СтруктураЗапроса.Вставить("model", ТекМодель);
	СтруктураЗапроса.Вставить("messages", Новый Массив);
	СтруктураЗапроса.messages.Добавить(Новый Структура("role,content", "user", Текст));
	HTTPЗапрос.УстановитьТелоИзСтроки(ирОбщий.ОбъектВСтрокуJSONЛкс(СтруктураЗапроса));
	Попытка
		HTTPОтвет = HTTPСоединение.ОтправитьДляОбработки(HTTPЗапрос);
	Исключение
		ВызватьИсключение "Ошибка отправки запроса: " + ОписаниеОшибки();
	КонецПопытки; 
	ТелоОтвета = HTTPОтвет.ПолучитьТелоКакСтроку();
	Если HTTPОтвет.КодСостояния <> 200 Тогда
		ВызватьИсключение ирОбщий.СтрШаблонЛкс(
				"Ошибка API. Код: %1, Ответ: %2",
				HTTPОтвет.КодСостояния,
				ТелоОтвета
				);
	КонецЕсли;
	ОтветJSON = ирОбщий.ОбъектИзСтрокиJSONЛкс(ТелоОтвета);
	Если ОтветJSON.Свойство("error") Тогда
		ВызватьИсключение ирОбщий.СтрШаблонЛкс(
				"Ошибка API. Код: %1, Сообщение: %2",
				ОтветJSON.error.code,
				ТелоОтвета
				);
	КонецЕсли;
	Если ОтветJSON.choices.Количество() > 0 Тогда
		Возврат ОтветJSON.choices[0].message.content;
	Иначе
		Возврат "";
	КонецЕсли;
КонецФункции

Функция ГенерироватьОписаниеМетодаИИ(Знач ТекстМодуля, Знач ИмяМетода = "", Знач ШиринаГенерируемогоТекста = 180) Экспорт
	Если ПустаяСтрока(ИмяМетода) Тогда
		ТекстПромпта = ТекстЗапросаИИДляДокументированияМетода(ТекстМодуля,, ШиринаГенерируемогоТекста);
	Иначе
		ТекстПромпта = ТекстЗапросаИИДляДокументированияЧерезВесьМодуль(ТекстМодуля, ИмяМетода,, ШиринаГенерируемогоТекста);
	КонецЕсли;
	мПлатформа = ирКэш.Получить();
	ОтветМоделиСтр = ОтправитьЗапросOpenAI(ТекстПромпта, мПлатформа.КлючСервисаИИ(),, мПлатформа.МодельИИ());
	Если ПустаяСтрока(ОтветМоделиСтр) Тогда
		Возврат Неопределено;
	КонецЕсли;
	Возврат ирОбщий.ОбъектИзСтрокиJSONЛкс(ирОбщий.ТекстМеждуМаркерамиЛкс(ОтветМоделиСтр, "```json", "```")); // см. ПрочитатьJSON(ОбразецДокументацииМетодаЖСОН())
КонецФункции

