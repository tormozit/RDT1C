		Тогда
			ДобавкаПараметров = """, 0.1, Истина";
			СтрокаДобавка = СтрЗаменить(СтрокаДобавка, "(" + УказательКаретки + ")", "(""" + УказательКаретки + ДобавкаПараметров + ")");
			СмещениеКурсораВОкончании = СмещениеКурсораВОкончании - СтрДлина(ДобавкаПараметров);
			ВидПовторногоВызоваАвтодополнения = 1;
		ИначеЕсли Истина
			И мЯзыкПрограммы = 0
			И СтрокаРезультата.ТипСлова = ПеречТипСлова.Свойство
			И Не ЛиВнутриВыражения(СтруктураТипаКонтекста)
			И СтрокаРезультата.ТипЗначенияИндекс <> "Неопределено"
			И (Ложь 
				Или ПустаяСтрока(мТекущаяСтрокаКонец)
				Или СокрЛП(мТекущаяСтрокаКонец) = ";")
			И (Ложь
				Или Найти(СтрокаРезультата.ТипЗначенияИндекс, "Ссылка.")
				Или ЛиПримитивныйТип(СтрокаРезультата.ТипЗначенияИндекс)
				Или мПлатформа.ЛиИмяСистемногоПеречисления(СтрокаРезультата.ТипЗначенияИндекс))
		Тогда 
			// Левая часть присвоения
			Если Ложь
				Или СтрокаРезультата.Определение = "Статистический" 
				Или СтрокаРезультата.Определение = "Локальный"
			Тогда
				ДоступноИзменение = Истина;
			Иначе
				ПараметрыЗаполнения = мПлатформа.НовыеПараметрыЗаполненияСлов(ПеречТипСлова.Свойство, БазовоеРасширениеКонфигурации(),,,, Ложь,,, Слово,,,, мЭтоЛокальныйКонтекстТаблицыСлов,, Истина, Истина);
				СловаРодителя = мПлатформа.ТаблицаСловИзСтруктурыТипа(СтруктураТипаКонтекста, ПараметрыЗаполнения); // - 3-4мс
				ДоступноИзменение = СловаРодителя.Количество() > 0;
			КонецЕсли;
			Если ДоступноИзменение Тогда
				СтрокаДобавка = СтрокаДобавка + " = " + УказательКаретки;
				Если ПустаяСтрока(мТекущаяСтрокаКонец) Тогда
					СтрокаДобавка = СтрокаДобавка + ";";
					СмещениеКурсораВОкончании = СмещениеКурсораВОкончании - 1;
				КонецЕсли;
				ВидПовторногоВызоваАвтодополнения = 1;
			КонецЕсли;
		КонецЕсли;
		мТекущаяСтрокаНачало = Лев(мТекущаяСтрокаНачало, мКонечнаяКолонка - 1 - СтрДлина(мНачалоКонтекста)) + СтрокаДобавка;
		ТекущаяСтрока = мТекущаяСтрокаНачало + НеобрабатываемыйКонецСтроки;
		Если мОригинальныйТекст = "" Тогда
			ВыделенныйТекст("" + ТекущаяСтрока);
		Иначе
			Если ПолеТекста.КоличествоСтрок() < мКонечнаяСтрока Тогда
				ПолеТекста.ДобавитьСтроку(ТекущаяСтрока);
			Иначе
				ПолеТекста.ЗаменитьСтроку(мКонечнаяСтрока, ТекущаяСтрока); // Баг платформы. Вызывает выделение всего предшествующего текста на 8.3.18
			КонецЕсли; 
		КонецЕсли;
		Если ВидПовторногоВызоваАвтодополнения > 0 Тогда
			СтрокаДобавка = СтрЗаменить(СтрокаДобавка, УказательКаретки, "<?>");
		КонецЕсли;
		Результат = СтрокаДобавка;
		мКонечнаяКолонка = мКонечнаяКолонка + СтрДлина(СтрокаДобавка) - СтрДлина(мНачалоКонтекста) - СтрДлина(СтрокаОкончания) + СмещениеКурсораВОкончании;
		мНачальнаяКолонка = мКонечнаяКолонка;
		мНачалоКонтекста = НовоеВыражение + ".";
		мНачалоСлова = "";  
	КонецЕсли;
	Если ВидПовторногоВызоваАвтодополнения = 0 Тогда
		Результат = СтрЗаменить(Результат, "<?>", УказательКаретки);
	КонецЕсли;
	мОткрытьСправкуПоПараметру = АвтоматическаяПодсказкаПоВызовуМетода() И СмещениеКурсораВОкончании = 1;
	Возврат Результат;
КонецФункции

// .
// Параметры:
//   МассивВставки -  - 
//   ОписаниеОжидаемогоТипа -  - 
//   УказательКаретки -  - 
//   ОсновнойКонецСтроки -  - 
//   СловаЛевые - ТаблицаЗначений - с/без колонки ТаблицаТипов
//   СловаПравые - ТаблицаЗначений - 
//   ШаблонИнструкции -  - 
Процедура СгенерироватьИнструкцииСопоставленияСвойств(Знач МассивВставки, Знач ВыражениеПриемник = "", Знач ОсновнойКонецСтроки, Знач СловаЛевые, Знач СловаПравые, Знач ШаблонИнструкции)
	Если СловаЛевые.Количество() = 0 Тогда
		Возврат
	КонецЕсли;
	СловаЛевые.Сортировать("Слово");
	Если ЗначениеЗаполнено(ВыражениеПриемник) Тогда
		СтрокаПервогоСлова = СловаЛевые.Найти(ирОбщий.ПоследнийФрагментЛкс(ВыражениеПриемник));
		СловаЛевые.Сдвинуть(СтрокаПервогоСлова, -СловаЛевые.Индекс(СтрокаПервогоСлова));
	КонецЕсли;
	Для Каждого СтрокаСлова Из СловаЛевые Цикл
		УточнитьТипЗначенияВСтрокеТаблицыСлов(СтрокаСлова);
	КонецЦикла;
	Для Каждого СтрокаСлова Из СловаПравые Цикл
		УточнитьТипЗначенияВСтрокеТаблицыСлов(СтрокаСлова);
	КонецЦикла;
	СловаПравые.Сортировать("Слово");
	Если СловаПравые.Колонки.Найти("НСлово") = Неопределено Тогда
		ирОбщий.ОбновитьКопиюКолонкиВНижнемРегистреЛкс(СловаПравые, "Слово");
	КонецЕсли;
	НаборыСлов = Новый Структура;
	НаборыСлов.Вставить("Основной", СловаПравые);
	Для Каждого СтрокаЛевогоСлова Из СловаЛевые Цикл
		КонецСтроки = ОсновнойКонецСтроки;
		СтрокаТекста = ирОбщий.СтрШаблонЛкс(ШаблонИнструкции, СтрокаЛевогоСлова.Слово);
		ПодобранноеПравоеСлово = "1/0"; // Для ошибки компиляции
		мСтрокаЛучшегоСлова = Неопределено;
		КлючПоиска = Новый Структура("НСлово", НРег(СтрокаЛевогоСлова.Слово));
		ЕстьТип = СтрокаЛевогоСлова.ТипЗначенияИндекс <> "?";
		Если ЕстьТип Тогда
			КлючПоиска.Вставить("ТипЗначенияИндекс", СтрокаЛевогоСлова.ТипЗначенияИндекс);
		КонецЕсли;
		Если Не ОбозначитьСловоСОжидаемымТипомВТаблицеСлов(КлючПоиска, НаборыСлов, "Основной") И ЕстьТип Тогда
			КлючПоиска = Новый Структура("ТипЗначенияИндекс", СтрокаЛевогоСлова.ТипЗначенияИндекс);
			ОбозначитьСловоСОжидаемымТипомВТаблицеСлов(КлючПоиска, НаборыСлов, "Основной",,, СтрокаЛевогоСлова.Слово);
		КонецЕсли;
		Если мСтрокаЛучшегоСлова <> Неопределено Тогда
			ПодобранноеПравоеСлово = мСтрокаЛучшегоСлова.Слово;
		КонецЕсли;
		МассивВставки.Добавить(СтрокаТекста + ПодобранноеПравоеСлово + КонецСтроки);
	КонецЦикла;
КонецПроцедуры

// Предварительно нужно вызывать РассчитатьОжидаемыйТипВыражения
Функция СоздатьПеременнуюИзВыделенногоВыражения(Знач РазрешитьДиалоги = Ложь, Знач ВыделенныйТекст = "") Экспорт
	АнализаторЛокальный = АнализаторСПолнойТаблицейСловМетода();
	БазовоеИмя = "Переменная";
	Если мКэшОжидаемогоТипа <> Неопределено Тогда
		ВыделенныйТекст = мКэшОжидаемогоТипа.ВыделенныйТекст;
		БазовоеИмя = ирОбщий.ПоследнийФрагментЛкс(мКэшОжидаемогоТипа.ВыражениеПриемник);
		Если Ложь
			Или ПустаяСтрока(БазовоеИмя)
			Или (Истина
				И ирОбщий.ЛиИмяПеременнойЛкс(мКэшОжидаемогоТипа.ВыражениеПриемник) 
				И мКэшОжидаемогоТипа.ПредшествующийТокен = "=")
		Тогда
			ВариантыИмени = Новый Массив;
			ТаблицаТипов = мКэшОжидаемогоТипа.ТаблицаОжидаемыхТипов;
			Если Не ЗначениеЗаполнено(ТаблицаТипов) Тогда
				ТаблицаТипов = ВычислитьТипЗначенияВыражения(ВыделенныйТекст);
			КонецЕсли;
			ДобавитьВариантыИмениНовойПеременнойИзТипа(ВариантыИмени, ТаблицаТипов);
			Если Истина
				И ВариантыИмени.Количество()
				И (Ложь
					Или ПустаяСтрока(БазовоеИмя)
					Или ВариантыИмени[0] <> "Произвольный")
			Тогда
				БазовоеИмя = ВариантыИмени[0];
			КонецЕсли;
		КонецЕсли; 
	КонецЕсли;
	ИмяПеременной = АнализаторЛокальный.НовоеИмяПеременной(БазовоеИмя);
	Если РазрешитьДиалоги Тогда
		Если Не ВвестиСтроку(ИмяПеременной, "Введите имя новой переменной") Тогда
			Возврат Неопределено;
		КонецЕсли;
	КонецЕсли;
	НачалоСтрокиСВыделенным = мТекущаяСтрокаНачало;
	Если ПустаяСтрока(ВыделенныйТекст) Тогда
		ЛучшийТип = мКэшОжидаемогоТипа.ТаблицаОжидаемыхТипов[0];
		Если ТипЗнч(ЛучшийТип.СтрокаОписания) = Тип("СтрокаТаблицыЗначений") Тогда
			СтрокаТипа = ЛучшийТип.СтрокаОписания;
		Иначе
			СтрокаТипа = СтрокаОбщегоТипаСКонструктором(ЛучшийТип.ИмяОбщегоТипа, ИмяПеременной);
		КонецЕсли;
		ИнструкцияПрисвоения = СоздатьИнструкциюУстановкиПеременной(СтрокаТипа, ИмяПеременной);
		НачалоСтрокиБезВыделенного = НачалоСтрокиСВыделенным;
	Иначе
		НачалоСтрокиБезВыделенного = ирОбщий.ПервыйФрагментЛкс(НачалоСтрокиСВыделенным, ВыделенныйТекст);
		ИнструкцияПрисвоения = ИмяПеременной + " = " + ВыделенныйТекст;
	КонецЕсли;
	МассивСтрок = Новый Массив;
	НомерСтроки = мНачальнаяСтрока;
	ТекущаяСтрока = НачалоСтрокиСВыделенным + ИмяПеременной;
	ТекущаяСтрокаЛ = СокрЛ(ТекущаяСтрока);
	Пока Ложь
		Или ирОбщий.СтрНачинаетсяСЛкс(ТекущаяСтрокаЛ, "Или ")
		Или ирОбщий.СтрНачинаетсяСЛкс(ТекущаяСтрокаЛ, "И ")
		Или Найти("(,-+<>/|", Лев(ТекущаяСтрокаЛ, 1)) > 0
	Цикл
		МассивСтрок.Вставить(0, ТекущаяСтрока);
		НомерСтроки = НомерСтроки - 1;
		Если НомерСтроки = 0 Тогда
			Прервать;
		КонецЕсли;
		ТекущаяСтрока = ПолеТекста.ПолучитьСтроку(НомерСтроки);
		ТекущаяСтрокаЛ = СокрЛ(ТекущаяСтрока);
	КонецЦикла;
	МассивСтрок.Вставить(0, ТекущаяСтрока);
	Результат =
		ирОбщий.НепечатноеНачалоЛкс(мТекущаяСтрокаНачало) + ИнструкцияПрисвоения + ";
		|" + ирОбщий.СтрСоединитьЛкс(МассивСтрок, Символы.ПС);
	//Если мКэшОжидаемогоТипа.ПредшествующийТокен = "=" Тогда
	//	Результат = Результат + ";";
	//КонецЕсли;
	УстановитьГраницыВыделения(НомерСтроки, 1, мКонечнаяСтрока, мКонечнаяКолонка);
	ВыделенныйТекст("");
	ПолучитьГраницыВыделения();
	ПрочитатьНачалоИКонецТекущейСтроки();
	ВставитьУказанныйШаблон(Результат, НачалоСтрокиСВыделенным);
	Возврат Результат;
КонецФункции

//.
// Возвращаемое значение:
//   ОбработкаОбъект.ирКлсПолеТекстаПрограммы - 
Функция АнализаторСПолнойТаблицейСловМетода(Знач НаНачалоТекущейИнструкции = Ложь, Знач ЗапретГлобальногоКонтекста = Ложь) Экспорт
	АнализаторЛокальный = КопияКомпоненты(); 
	Если ФормаВладелец <> Неопределено Тогда
		// Отвязываем от интерактивного поля
		АнализаторЛокальный.мСлужебнаяФорма = мПлатформа.ПолучитьФорму("Служебная");
		АнализаторЛокальный.СоздатьПолеТекста();
		АнализаторЛокальный.ПолеТекста.УстановитьТекст(мОригинальныйТекст);
	КонецЕсли;
	Если НаНачалоТекущейИнструкции Тогда
		АнализаторЛокальный.УстановитьТекущуюПозициюВНачалоИнструкции();
	Иначе
		АнализаторЛокальный.УстановитьТекущуюПозициюВКонецБлока();
	КонецЕсли;
	АнализаторЛокальный.КончитьОбработкуКоманды();
	АнализаторЛокальный.ЗаполнитьТаблицуСлов(, Ложь,, Ложь, Ложь,,,, ЗапретГлобальногоКонтекста);
	Возврат АнализаторЛокальный;
КонецФункции

// Рекомендуется вызывать через АнализаторСПолнойТаблицейСловМетода()
Функция НовоеИмяПеременной(Знач БазовоеИмя = "") Экспорт
	Если ПустаяСтрока(БазовоеИмя) Тогда
		БазовоеИмя = "Переменная";
	КонецЕсли;
	Результат = ирОбщий.АвтоУникальноеИмяВКоллекцииЛкс(ТаблицаСлов, БазовоеИмя, "Слово",,,,, Новый Структура("ТипСлова", ПеречТипСлова.Свойство));
	Возврат Результат;
КонецФункции

//.
// Параметры:
//   СтрокаСлова - СтрокаТаблицыЗначений, ОбработкаТабличнаяЧастьСтрока.ирКлсПолеТекстаПрограммы.ТаблицаСлов - 
Процедура ОбновитьСтатистикуВыбораВСтрокеСлова(Знач СтрокаСлова)
	Если Ложь
		Или СтрокаСлова.Определение = "Локальный"
		Или СтрокаСлова.Определение = "Статистический"
		//Или СтрокаСлова.ТипСлова = ПеречТипСлова.Конструкция
		//Или ЛиБесполезноеПрисвоениеПеременной(СтрокаСлова.Слово)
		//Или ирОбщий.ЛиБулевыйЛитералЛкс(СтрокаСлова.Слово)
	Тогда
		Возврат;
	КонецЕсли;
	ТипВладелец = Неопределено; // см. мПлатформа.НоваяСтруктураТипа
	Если Истина
		И СтрокаСлова.Определение = ПеречИсточникСлова.Предопределенный
		И (Ложь
			Или СтрокаСлова.ТипСлова = ПеречТипСлова.Конструкция
			Или ЛиБесполезноеПрисвоениеПеременной(СтрокаСлова.Слово)
			Или ирОбщий.ЛиБулевыйЛитералЛкс(СтрокаСлова.Слово))
	Тогда
		ТипВладелец = НоваяСтруктураТипа("Глобальный");
	Иначе
		УточнитьТипЗначенияВСтрокеТаблицыСлов(СтрокаСлова, Истина,, ТипВладелец);
	КонецЕсли;
	ТипКонтекстаСтатистики = ТипКонтекстаСтатистики(ТипВладелец);
	Если ТипКонтекстаСтатистики <> Неопределено Тогда
		КлючПоиска = Новый Структура("ЯзыкПрограммы, ТипКонтекста, Слово, ЭтоМетод", ЯзыкПрограммы, ТипКонтекстаСтатистики, СтрокаСлова.Слово, СтрокаСлова.ТипСлова = ПеречТипСлова.Метод);
		ТаблицаСтатистикиВыбора = мПлатформа.ТаблицаСтатистикиВыбора;
		НайденныеСтроки = ТаблицаСтатистикиВыбора.НайтиСтроки(КлючПоиска);
		Если НайденныеСтроки.Количество() > 0 Тогда
			СтрокаСтатистикиВыбора = НайденныеСтроки[0];
		Иначе
			СтрокаСтатистикиВыбора = ТаблицаСтатистикиВыбора.Добавить();
			ЗаполнитьЗначенияСвойств(СтрокаСтатистикиВыбора, КлючПоиска);
		КонецЕсли;
		СтрокаСтатистикиВыбора.Рейтинг = СтрокаСтатистикиВыбора.Рейтинг + 1;
	КонецЕсли;
КонецПроцедуры 

Функция ТипКонтекстаСтатистики(Знач СтруктураТипаКонтекста = Неопределено) Экспорт
	Если СтруктураТипаКонтекста = Неопределено Тогда
		СтруктураТипаКонтекста = мСтруктураТипаКонтекста; 
	КонецЕсли;
	ТипКонтекстаСтатистики = Неопределено;
	КонкретныйТипКонтекста = мПлатформа.ИмяТипаИзСтруктурыТипа(СтруктураТипаКонтекста);
	Если СтруктураТипаКонтекста.ИмяОбщегоТипа = "Локальный" Тогда
		ТипКонтекстаСтатистики = "Глобальный";
	ИначеЕсли Ложь
		Или СтруктураТипаКонтекста.ТипЯзыка = "ИмяТипа"
		Или СтруктураТипаКонтекста.ТипЯзыка = "ЗначениеВЗапросе"
		Или СтруктураТипаКонтекста.ТипЯзыка = "ИмяПредопределенногоЗначения"
	Тогда
		ТипКонтекстаСтатистики = мРодительскийКонтекст;
		Если Не ЗначениеЗаполнено(ТипКонтекстаСтатистики) Тогда
			ТипКонтекстаСтатистики = "<" + СтруктураТипаКонтекста.ТипЯзыка + ">";
		КонецЕсли; 
	ИначеЕсли Истина
		И Не мЭтоТекстовыйЛитерал
		//И мЯзыкПрограммы = 0 // Отключил, т.к. выше исключаются все неподходящие случаи через ТипЯзыка 
		И (Ложь
			Или Найти(КонкретныйТипКонтекста, ".") > 0
			Или Найти(КонкретныйТипКонтекста, "[") > 0)
	Тогда
		//ТипКонтекстаСтатистики = ИмяТипаКонтекста(); 
		ТипКонтекстаСтатистики = КонкретныйТипКонтекста; 
	ИначеЕсли Ложь
		Или ТаблицаСлов.Найти("Метаданные", "Определение") = Неопределено
		Или Не мЭтоТекстовыйЛитерал И СтруктураТипаКонтекста.Метаданные = Неопределено // для СправочникиМенеджер
	Тогда
		ТипКонтекстаСтатистики = ирОбщий.ПервыйФрагментЛкс(КонкретныйТипКонтекста, " ");
	КонецЕсли;
	Возврат ТипКонтекстаСтатистики;
КонецФункции 

